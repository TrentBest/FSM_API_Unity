*.pidb
*.pidb.meta
*.pdb
*.pdb.meta
*.mdb
*.mdb.meta
*.tmp
*.svd
*.VC.db
*.opendb

# Unity IDE meta/cache
.vs/
.gradle/
ExportedObj/
.consulo/
*.user
*.userprefs
*.suo
*.booproj
*.csproj
*.unityproj
*.sln
Packages/

# Crashlytics build data
crashlytics-build.properties

# Blender junk
*.blend1
*.blend1.meta

# Android / iOS / Mac Builds
*.apk
*.aab
*.app

